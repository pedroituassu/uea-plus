{
  "manifest_version" : 3,
  "name" : "UEA+",
  "version" : "1.1",

  "description": "O portal da UEA é uma MERDA e eu não aguento mais ter que logar toda vez que eu quero entrar então fiz isso",

  "content_scripts" : [
    {
      "matches" : ["*://*.uea.edu.br/app/login.php"],
      "js" : ["change_field.js"]
    }
  ],

  "permissions" : ["activeTab"],

  "browser_specific_settings": {
    "gecko": {
      "id": "uea-plus@uea.edu.br"
    }
  }
}